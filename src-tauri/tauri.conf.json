{
  "productName": "algokit-lora",
  "version": "0.1.0",
  "identifier": "algokit-lora.algorand-foundation",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "algokit-lora",
        "width": 1400,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": false,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  },
  "plugins": {
    "deep-link": {
      "mobile": [],
      "desktop": {
        "schemes": ["algokit-lora"]
      }
    },
    "cli": {
      "description": "algokit-lora"
    },
    "updater": {
      "endpoints": ["https://cdn.crabnebula.app/update/CN_ORG_NAME/CN_APP_NAME/{{target}}-{{arch}}/{{current_version}}CN_CHANNEL"],
      "pubkey": "TAURI_SIGNING_PUBLIC_KEY"
    }
  }
}
