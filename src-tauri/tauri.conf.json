{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "algokit-lora",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "algokit-lora",
        "width": 1400,
        "height": 800,
        "endpoint": "https://weu.codesigning.azure.net/",
        "trustedSigningAccountName": "algokit-signing",
        "certificateProfileName": "algokit-lora",
        "filesFolder": "./src-tauri/target/release/deps/algokit-lora.exe",
        "filesFolderFilter": "exe",
        "fileDigest": "SHA256",
        "timestampRfc3161": "http://timestamp.acs.microsoft.com",
        "timestampDigest": "SHA256"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.tauri-poc.pat",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
    }
  }
}
